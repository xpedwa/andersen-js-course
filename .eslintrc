{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "import/prefer-default-export": "off",
    "no-unused-vars": "warn",
    "class-methods-use-this": "warn"
  }
}